{
   "name": "car-rental-system",
   "version": "1.0.0",
   "description": "car rental system using mern stack tech",
   "main": "server.js",
   "type": "module",
   "scripts": {
      "start": "node server.js",
      "server": "nodemon server.js",
      "client": "npm run dev --prefix client",
      "dev": "concurrently \"npm run server\" \"npm run client\"",
      "build": "npm install && npm install --prefix client && npm run build --prefix client",
      "generate-toc": "markdown-toc -i readme.md"
   },
   "keywords": [
      "car-rental",
      "rental",
      "car",
      "motor-car",
      "motor",
      "rent",
      "drive"
   ],
   "author": "Naimat Ullah",
   "license": "ISC",
   "dependencies": {
      "bcrypt": "^5.1.1",
      "bcryptjs": "^2.4.3",
      "cookie-parser": "^1.4.6",
      "cors": "^2.8.5",
      "dotenv": "^16.4.5",
      "express": "^4.19.2",
      "express-rate-limit": "^7.4.0",
      "joi": "^17.13.3",
      "jsonwebtoken": "^9.0.2",
      "knex": "^3.1.0",
      "morgan": "^1.10.0",
      "pg": "^8.12.0",
      "redis": "^4.7.0",
      "slugify": "^1.6.6",
      "validator": "^13.12.0"
   },
   "devDependencies": {
      "concurrently": "^8.2.2",
      "nodemon": "^3.1.4"
   }
}
